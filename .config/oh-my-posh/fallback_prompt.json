{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#89B4FA",
    "darkerGray": "#434C5E",
    "darkestGray": "#3B4252",
    "g-blue": "#4285F4",
    "g-green": "#0F9D58",
    "g-red": "#DB4437",
    "g-yellow": "#F4B400",
    "lavender": "#B4BEFE",
    "lime": "#d2ff5e",
    "navy": "#193549",
    "orange": "#ff9248",
    "pink": "#F5C2E7",
    "red": "#BF616A",
    "reddish-orange": "#f26d50",
    "teal": "#89d1dc",
    "yellow": "#fed766",
    "yellowish-orange": "#f17c37"
  },
  "secondary_prompt": {
    "template": "\u2022 ",
    "foreground": "darkGray",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf141",
            "folder_separator_icon": " \ueab6 ",
            "home_icon": "\uf015",
            "mapped_locations": {
              "~/.config": "\ue615",
              "~/.config/nvim": "\ue62b",
              "~/.dotfiles": "\ue615",
              "~/.dotfiles/vim": "\ue62b",
              "~/Documents": "\udb80\ude19",
              "~/Documents/GitHub": "\uf09b",
              "~/Downloads": "\uf019"
            },
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "p:blue",
          "background": "p:darkerGray",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}{{ .Full }} {{ end }}{{ end }}",
          "foreground": "p:navy",
          "background": "p:yellow",
          "type": "python"
        },
        {
          "properties": {
            "branch_icon": "\uf418 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "p:navy",
          "background": "p:lime",
          "type": "git",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:orange{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:reddish-orange{{ end }}",
            "{{ if gt .Ahead 0 }}p:teal{{ end }}",
            "{{ if gt .Behind 0 }}p:yellowish-orange{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "alias": "Closer",
          "template": "\nâžœ",
          "foreground": "green",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
