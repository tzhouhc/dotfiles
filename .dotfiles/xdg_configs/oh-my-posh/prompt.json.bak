{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "pink": "#F5C2E7",
    "lavender": "#B4BEFE",
    "blue": "#89B4FA",
    "lime": "#d2ff5e",
    "navy": "#193549",
    "yellow": "#fed766",
    "red": "#BF616A",
    "orange": "#ff9248",
    "reddish-orange": "#f26d50",
    "teal": "#89d1dc",
    "yellowish-orange": "#f17c37",
    "g-blue": "#4285F4",
    "g-red": "#DB4437",
    "g-yellow": "#F4B400",
    "g-green": "#0F9D58",
    "darkerGray": "#434C5E",
    "darkestGray": "#3B4252"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:blue",
          "background": "p:darkerGray",
          "properties": {
            "folder_icon": "ÔÖÅ",
            "folder_separator_icon": " Ó™∂ ",
            "home_icon": "ÔÄï",
            "style": "agnoster_short",
            "mapped_locations": {
              "~/Documents/GitHub": "ÔÇõ",
              "~/Downloads": "ÔÄô",
              "~/Documents": "Û∞àô",
              "~/.dotfiles": "Óòï",
              "~/.config": "Óòï",
              "~/.config/nvim": "Óò´",
              "~/.dotfiles/vim": "Óò´"
            }
          },
          "style": "plain",
          "template": "‚ñé{{ .Path }} ",
          "type": "path"
        },
        {
          "type": "python",
          "background": "p:yellow",
          "foreground": "p:navy",
          "template": " Óàµ {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}{{ .Full }} {{ end }}{{ end }}"
        },
        {
          "background": "p:lime",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:orange{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:reddish-orange{{ end }}",
            "{{ if gt .Ahead 0 }}p:teal{{ end }}",
            "{{ if gt .Behind 0 }}p:yellowish-orange{{ end }}"
          ],
          "foreground": "p:navy",
          "properties": {
            "branch_icon": "Ôêò ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} Ó≠ã {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "p:pink",
          "type": "session",
          "template": "{{ if .SSHSession }}ÓÆ© {{ .HostName}}{{ end }}"
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}p:lime{{end}}",
            "{{if gt .Percentage 80}}p:g-green{{end}}",
            "{{if gt .Percentage 50}}p:g-blue{{end}}",
            "{{if gt .Percentage 30}}p:g-yellow{{end}}",
            "{{if gt .Percentage 0}}p:g-red{{end}}"
          ],
          "templates_logic": "first_match",
          "templates": [
            "{{ if eq \"Charging\" .State.String }}[Û±ü¶]{{end}}",
            "{{ if gt .Percentage 95 }}[Û∞Åπ]{{end}}",
            "{{ if gt .Percentage 90 }}[Û∞ÇÇ]{{end}}",
            "{{ if gt .Percentage 80 }}[Û∞ÇÅ]{{end}}",
            "{{ if gt .Percentage 70 }}[Û∞ÇÄ]{{end}}",
            "{{ if gt .Percentage 60 }}[Û∞Åø]{{end}}",
            "{{ if gt .Percentage 50 }}[Û∞Åæ]{{end}}",
            "{{ if gt .Percentage 40 }}[Û∞ÅΩ]{{end}}",
            "{{ if gt .Percentage 30 }}[Û∞Åº]{{end}}",
            "{{ if gt .Percentage 20 }}[Û∞Åª]{{end}}",
            "{{ if gt .Percentage 10 }}[Û∞Å∫]{{end}}"
          ]
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "alias": "Closer",
          "foreground": "green",
          "template": "\n‚ûú",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:yellow",
          "foreground": "p:navy",
          "type": "executiontime",
          "template": " Ôê∫ {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500,
            "style": "austin"
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:yellow",
          "background": "p:darkerGray",
          "template": " {{ .CurrentDate | date .Format }}üÆá",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ],
      "type": "rprompt"
    }
  ],
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "darkGray",
    "template": "‚Ä¢ "
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "darkGray",
    "template": "{{ .Segments.Time.CurrentDate | date .Segments.Time.Format }} {{ if gt .Segments.Executiontime.Ms 500 }}+<#ebcb8b>{{ .Segments.Executiontime.FormattedMs }} {{end}}{{ if gt .Code 0 }}<#bf616a>{{ .Code }} Ôë§ {{ else }}<#a3be8c>Ôë§ {{ end }}"
  },
  "final_space": true,
  "version": 2
}
