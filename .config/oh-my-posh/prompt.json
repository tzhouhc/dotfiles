{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#89B4FA",
    "darkerGray": "#434C5E",
    "darkestGray": "#3B4252",
    "g-blue": "#4285F4",
    "g-green": "#0F9D58",
    "g-red": "#DB4437",
    "g-yellow": "#F4B400",
    "lavender": "#B4BEFE",
    "lime": "#d2ff5e",
    "navy": "#193549",
    "orange": "#ff9248",
    "pink": "#F5C2E7",
    "red": "#BF616A",
    "reddish-orange": "#f26d50",
    "teal": "#89d1dc",
    "yellow": "#fed766",
    "yellowish-orange": "#f17c37"
  },
  "secondary_prompt": {
    "template": "\u2022 ",
    "foreground": "darkGray",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "{{ if .Segments.Time.CurrentDate }}{{ .Segments.Time.CurrentDate | date .Segments.Time.Format }} {{ if gt .Segments.Executiontime.Ms 500 }}+<#ebcb8b>{{ .Segments.Executiontime.FormattedMs }} {{end}}{{ if gt .Code 0 }}<#bf616a>{{ .Code }} \uf464 {{ else }}<#a3be8c>\uf464 {{ end }}{{ end }}",
    "foreground": "darkGray",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf141",
            "folder_separator_icon": " \ueab6 ",
            "home_icon": "\uf015",
            "mapped_locations": {
              "~/.config": "\ue615",
              "~/.config/nvim": "\ue62b",
              "~/.dotfiles": "\ue615",
              "~/.dotfiles/vim": "\ue62b",
              "~/Documents": "\udb80\ude19",
              "~/Documents/GitHub": "\uf09b",
              "~/Downloads": "\uf019"
            },
            "style": "agnoster_short"
          },
          "template": "\u258e{{ .Path }} ",
          "foreground": "p:blue",
          "background": "p:darkerGray",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}{{ .Full }} {{ end }}{{ end }}",
          "foreground": "p:navy",
          "background": "p:yellow",
          "type": "python"
        },
        {
          "properties": {
            "branch_icon": "\uf418 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "p:navy",
          "background": "p:lime",
          "type": "git",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:orange{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:reddish-orange{{ end }}",
            "{{ if gt .Ahead 0 }}p:teal{{ end }}",
            "{{ if gt .Behind 0 }}p:yellowish-orange{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .SSHSession }}\ueba9 {{ .HostName}}{{ end }}",
          "foreground": "p:pink",
          "type": "session"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "templates_logic": "first_match",
          "type": "battery",
          "style": "plain",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}p:lime{{end}}",
            "{{if gt .Percentage 80}}p:g-green{{end}}",
            "{{if gt .Percentage 50}}p:g-blue{{end}}",
            "{{if gt .Percentage 30}}p:g-yellow{{end}}",
            "{{if gt .Percentage 0}}p:g-red{{end}}"
          ],
          "templates": [
            "{{ if eq \"Charging\" .State.String }}[\udb85\udfe6]{{end}}",
            "{{ if gt .Percentage 95 }}[\udb80\udc79]{{end}}",
            "{{ if gt .Percentage 90 }}[\udb80\udc82]{{end}}",
            "{{ if gt .Percentage 80 }}[\udb80\udc81]{{end}}",
            "{{ if gt .Percentage 70 }}[\udb80\udc80]{{end}}",
            "{{ if gt .Percentage 60 }}[\udb80\udc7f]{{end}}",
            "{{ if gt .Percentage 50 }}[\udb80\udc7e]{{end}}",
            "{{ if gt .Percentage 40 }}[\udb80\udc7d]{{end}}",
            "{{ if gt .Percentage 30 }}[\udb80\udc7c]{{end}}",
            "{{ if gt .Percentage 20 }}[\udb80\udc7b]{{end}}",
            "{{ if gt .Percentage 10 }}[\udb80\udc7a]{{end}}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "alias": "Closer",
          "template": "\n{{ if eq .Code 0 }}➜{{ else }}\u258e{{ .Code }} ➜{{ end }}",
          "foreground_templates": ["{{ if eq .Code 0 }}green{{ else }}red{{ end }}"],
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "template": " \uf43a {{ .FormattedMs }} ",
          "foreground": "p:navy",
          "background": "p:yellow",
          "type": "executiontime"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "template": " {{ .CurrentDate | date .Format }}\ud83e\udf87",
          "foreground": "p:yellow",
          "background": "p:darkerGray",
          "type": "time",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 4,
  "final_space": true
}
